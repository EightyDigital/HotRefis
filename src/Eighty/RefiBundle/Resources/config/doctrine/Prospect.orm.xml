<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Eighty\RefiBundle\Entity\Prospect" table="prospect">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="amicusPersonId" type="bigint" column="amicus_person_id" nullable="true"/>
    <field name="firstname" type="string" column="firstname" length="255" nullable="true"/>
    <field name="lastname" type="string" column="lastname" length="255" nullable="true"/>
    <field name="age" type="integer" column="age" nullable="true"/>
    <field name="contact" type="string" column="contact" length="25" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="address" type="text" column="address" nullable="true"/>
    <field name="postcode" type="string" column="postcode" length="6" nullable="true"/>
    <field name="profession" type="string" column="profession" length="255" nullable="true"/>
    <field name="districtcode" type="integer" column="districtcode" nullable="true"/>
    <field name="derivedIncome" type="float" column="derived_income" precision="10" scale="0" nullable="true"/>
    <field name="isdnc" type="integer" column="isDNC" nullable="true"/>
    <field name="lastContact" type="datetime" column="last_contact" nullable="true"/>
    <field name="lastRefinance" type="datetime" column="last_refinance" nullable="true"/>
  </entity>
</doctrine-mapping>
