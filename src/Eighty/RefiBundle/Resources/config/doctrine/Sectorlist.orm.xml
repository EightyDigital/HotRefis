<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Eighty\RefiBundle\Entity\Sectorlist" table="sectorlist">
    <id name="id" type="tinyint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="clientId" type="tinyint" column="client_id" length="11" nullable="true"/>
    <field name="sectorCode" type="string" column="sector_code" length="4" nullable="true"/>
	<field name="dateadded" type="string" column="dateadded" length="20" nullable="true"/>
    <field name="validity" type="tinyint" column="validity" length="4" nullable="true"/>
  </entity>
</doctrine-mapping>
