<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Eighty\RefiBundle\Entity\Hometracker" table="hometracker">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="50" nullable="true"/>
    <field name="password" type="string" column="password" length="50" nullable="true"/>
    <field name="prospectId" type="bigint" column="prospect_id" nullable="true"/>
    <field name="contact" type="string" column="contact" length="20" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="address" type="text" column="address" nullable="true"/>
    <field name="postcode" type="string" column="postcode" length="6" nullable="true"/>
    <field name="referrer" type="bigint" column="referrer" nullable="true"/>
  </entity>
</doctrine-mapping>
