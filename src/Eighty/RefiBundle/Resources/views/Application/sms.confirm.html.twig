{% extends "RefiBundle::tempBase.html.twig" %}

{% block header %}
{% endblock header %}





{% block content %}
{# <div ng-controller="map__controller" class="map__container" >
  <div id="heatmap" class="angular-google-map-container"></div>
</div> #}


<div class="container" style="clear:both; margin-top:25px;">
  <div class="row-fluid">
    <div class="span12">
      <h2 style="font-size: 32px;
         font-weight: 400;
         color: #555;
         margin: 15px  0;
         text-transform: uppercase;">You have sent your list</h2>
    </div>
    <div class="span12">
      <p>You have blasted to <span>{{ prospects }}</span> prospects over <span>{{ properties }}</span> properties</p>
	  
	  <br/>
	  
	  <table border="1">
		<thead>
			<th>Project Name</th>
			<th># of Blasts</th>
		</thead>
		<tbody>
			{% for transaction in transactions %}
				<tr>
					<td>{{ transaction.name }}</td>
					<td>{{ transaction.count }}</td>
				</tr>
			{% endfor %}
		</tbody>
	  </table>
	</div>
  </div>
</div>
{% endblock content %}





{% block footer %}
  {{ parent() }}
{% endblock footer %}

