<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie10 lt-ie9 lt-ie8 is-ie7"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie10 lt-ie9 is-ie8"> <![endif]-->
<!--[if IE 9]>         <html class="no-js lt-ie10 is-ie9"> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" ng-app="refis" ><!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>{% block title %}HotRefis{% endblock title %}</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="canonical" href="http://www.transactor.propertyguru.com.sg">
  <!-- Icons -->
  <link rel="shortcut icon" href="/bundles/images/refi/favicon.png">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon57.png" sizes="57x57">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon72.png" sizes="72x72">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon76.png" sizes="76x76">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon114.png" sizes="114x114">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon120.png" sizes="120x120">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon144.png" sizes="144x144">
  <link rel="apple-touch-icon" href="/bundles/images/refi/icon152.png" sizes="152x152">
  <!-- END Icons -->

  <!-- Default Open Sans! -->
  <link href='//fonts.googleapis.com/css?family=Open+Sans:300,400,600,800' rel='stylesheet' type='text/css'>

  {% block stylesheets %}

    {% stylesheets filter="?yui_css"
      "@RefiBundle/Resources/assets/css/base.css"
      "@RefiBundle/Resources/assets/css/hotrefi.css"
      %}
      <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <!--[if IE]>
      {% stylesheets filter="?yui_css"
        '@RefiBundle/Resources/assets/css/ie.css'
      %}
        <style href="{{ asset_url }}" rel="stylesheet" type="text/css"></style>
      {% endstylesheets %}

    <![endif]-->

    <!--[if IE 7]>
      {% stylesheets filter="?yui_css"
        '@RefiBundle/Resources/assets/css/font-awesome-ie7.css'
      %}
        <style href="{{ asset_url }}" rel="stylesheet" type="text/css"></style>
      {% endstylesheets %}
    <![endif]-->
  {% endblock stylesheets %}

  {# jQuery CDN || Load from local #}
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/jquery-1.10.2.min.js'
    %}
    <script>window.jQuery || document.write("<script src=\"{{ asset_url }}\"><\/script>")</script>
  {% endjavascripts %}

  <!-- Jquery UI -->
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/jquery-ui.min.js'
  %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}


  {# Modernizer CDN || Load from local #}
  <script src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.2/modernizr.min.js"></script>
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/modernizr-2.8.2.min.js'
    %}
    <script>Modernizr.load || document.write("<script src=\"{{ asset_url }}\"><\/script>")</script>
  {% endjavascripts %}



  {# ANGULARRRRGH #}
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/angular.js'
  %}
     <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {#<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.20/angular.min.js"></script>#}


  {# Under SHHHCORE! #}
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/lodash.min.js'
  %}
     <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

  {# ANGULARRRRGH Maps #}
  <script src='//maps.googleapis.com/maps/api/js?libraries=visualization&sensor=false"'></script>
  {% javascripts
    '@RefiBundle/Resources/assets/js/vendor/angular-google-maps.min.js'
  %}
     <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

</head>


<body class="{% block bodyclass %}{% endblock bodyclass %}">
  <header id="hd">
    {% include "RefiBundle:Application:refi.header.html.twig" %}
  </header>

  <section id="bd">
    {% block content %}
    {% endblock content %}
  </section>

  <footer id="ft">
    {% block footer %}
    {% endblock footer %}
  </footer>

  {% block javascripts %}
    {% javascripts filter="?yui_js"
      "@RefiBundle/Resources/assets/js/plugins.js"
      "@RefiBundle/Resources/assets/js/vendor/xeditable.min.js"
      "@RefiBundle/Resources/assets/js/vendor/charts.min.js"
      "@RefiBundle/Resources/assets/js/vendor/charts-ng.js"
      "@RefiBundle/Resources/assets/js/vendor/angular-slider-ui.js"
      "@RefiBundle/Resources/assets/js/refimap.js"
      "@RefiBundle/Resources/assets/js/main.js"
      %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock javascripts %}

</body>
</html>
